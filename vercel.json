{
  "version": 2,
  "builds": [
    {
      "src": "api/simple_api.py",
      "use": "@vercel/python"
    },
    {
      "src": "package.json",
      "use": "@vercel/next",
      "config": {
        "distDir": ".next"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/simple_api.py"
    },
    {
      "src": "/(.*)",
      "dest": "/$1",
      "continue": true
    }
  ],
  "env": {
    "PYTHON_VERSION": "3.11"
  },
  "functions": {
    "api/simple_api.py": {
      "maxDuration": 10,
      "memory": 1024
    }
  },
  "regions": ["iad1"]
}