{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "package.json",
      "use": "@vercel/next",
      "config": {
        "distDir": ".next"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/$1",
      "continue": true
    }
  ],
  "functions": {
    "api/index.js": {
      "maxDuration": 10,
      "memory": 1024
    }
  },
  "regions": ["iad1"]
}